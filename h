x = 0
y = int(input("chislo"))
while x < y:
    x = x + 1
    if x == y:
        break
    print(x)

z = input('stroka')
print(z[::-1])

a = input('chislo')
for x in range(0, 10):
    print(a * x)
    
x = int(input('chislo'))
if x % 2 == 0:
    print('chotno')
elif x % 2 == 1:
    print('nechotno')    
else:
        print('zanovo') 


        